<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>Kind</key>
	<string>Xcode.IDEKit.TextSubstitutionFileTemplateKind</string>
	<key>Description</key>
	<string>A set of classes to start an app module (View, Controller, Router)</string>
	<key>Summary</key>
	<string>App module architecture scaffolding</string>
	<key>SortOrder</key>
	<string>1</string>
	<key>Platforms</key>
	<array>
		<string>com.apple.platform.iphoneos</string>
	</array>
	<key>Options</key>
	<array>
		<dict>
			<key>Identifier</key>
			<string>productName</string>
			<key>Required</key>
			<true/>
			<key>Name</key>
			<string>Module:</string>
			<key>Description</key>
			<string>The name of the module to create</string>
			<key>Type</key>
			<string>text</string>
			<key>Default</key>
			<string>MyModule</string>
		</dict>
		<dict>
			<key>Identifier</key>
			<string>viewType</string>
			<key>Required</key>
			<true/>
			<key>Name</key>
			<string>View type:</string>
			<key>Description</key>
			<string>The type of view to use</string>
			<key>Type</key>
			<string>popup</string>
			<key>Default</key>
			<string>Storyboard</string>
			<key>Values</key>
			<array>
				<string>Storyboard</string>
				<string>XIB file</string>
				<string>Code</string>
			</array>
		</dict>
		<dict>
			<key>Identifier</key>
			<string>SegueEntryPoint</string>
			<key>Name</key>
			<string>Use segue as entry point</string>
			<key>Description</key>
			<string>Will add a code snippet to use in the presenting module</string>
			<key>Type</key>
			<string>checkbox</string>
			<key>RequiredOptions</key>
			<dict>
				<key>viewType</key>
				<array>
					<string>Storyboard</string>
				</array>
			</dict>
			<key>Default</key>
			<string>false</string>
		</dict>
	</array>
</dict>
</plist>
